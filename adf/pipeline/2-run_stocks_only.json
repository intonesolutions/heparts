{
	"name": "run_stocks_only",
	"properties": {
		"activities": [
			{
				"name": "STOCK_MASTER",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "FtpReadSettings",
							"recursive": false,
							"wildcardFileName": "STOCK_MASTER-*.xml",
							"useBinaryTransfer": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "FTP_TEXT",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "blob_TEXT",
						"type": "DatasetReference",
						"parameters": {
							"folder": "intake",
							"filename": "stock_master.xml"
						}
					}
				]
			},
			{
				"name": "call pass stock",
				"type": "AzureFunctionActivity",
				"dependsOn": [
					{
						"activity": "STOCK_MASTER",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"functionName": "HttpTrigger1",
					"method": "POST",
					"body": {
						"pass": "stock"
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureFunction1",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "products",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "call pass stock",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": false,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "SalesforceSink",
						"writeBatchSize": 5000,
						"writeBehavior": "upsert",
						"externalIdFieldName": "Product_Key__c",
						"ignoreNullValues": true
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['Id']"
								},
								"sink": {
									"name": "Id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['IsActive']"
								},
								"sink": {
									"name": "IsActive",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "$['SKU__c']"
								},
								"sink": {
									"name": "SKU__c",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Stock_Code__c']"
								},
								"sink": {
									"name": "Stock_Code__c",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Name']"
								},
								"sink": {
									"name": "Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Description']"
								},
								"sink": {
									"name": "Description",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Part_num__c']"
								},
								"sink": {
									"name": "Part_num__c",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Crown_Standard_Price__c']"
								},
								"sink": {
									"name": "Crown_Standard_Price__c",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['OEM_Retail_Price__c']"
								},
								"sink": {
									"name": "OEM_Retail_Price__c",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['Product_Key__c']"
								},
								"sink": {
									"name": "Product_Key__c",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Family']"
								},
								"sink": {
									"name": "Family",
									"type": "String"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "blob_json",
						"type": "DatasetReference",
						"parameters": {
							"folder": "stage",
							"filename": "product2.json"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "salesforce_OBJ",
						"type": "DatasetReference",
						"parameters": {
							"objName": "Product2"
						}
					}
				]
			},
			{
				"name": "delete_ftp_copy1",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "products",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "FTP_BIN",
						"type": "DatasetReference",
						"parameters": {
							"folder": "/"
						}
					},
					"enableLogging": false,
					"storeSettings": {
						"type": "FtpReadSettings",
						"recursive": false,
						"wildcardFileName": "STOCK_*.xml",
						"useBinaryTransfer": true
					}
				}
			}
		],
		"folder": {
			"name": "Pronto_SF"
		},
		"annotations": []
	}
}